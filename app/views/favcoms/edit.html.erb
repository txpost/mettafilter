<h1>Editing favcom</h1>

<% form_for(@favcom) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :comment_id %><br />
    <%= f.text_field :comment_id %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @favcom %> |
<%= link_to 'Back', favcoms_path %>